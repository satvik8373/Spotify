{
  "name": "Mavrixfy",
  "short_name": "Mavrixfy",
  "description": "A premium music experience with real-time features and enhanced music discovery",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#121212",
  "theme_color": "#121212",
  "orientation": "portrait-primary",
  "categories": ["music", "entertainment", "lifestyle"],
  "lang": "en",
  "dir": "ltr",
  "scope": "/",
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+music",
      "url": "/play?url=%s"
    }
  ],
  "icons": [
